package ch07;

public class SmartPhone extends Phone{
	public boolean wifi;
	
	public SmartPhone() {
		super();
		System.out.println("SmartPhone의 기본생성자");
	}
	
	public void setWifi(boolean wifi) {
		this.wifi = wifi;
		System.out.println("와이파이 상태를 변경합니다.");
	}
	public void internet() {
		System.out.println("인터넷에 연결합니다.");
	}
}

package ch06.example;

public class Exercise6_22 {

	private static boolean isNumber(String str) {
		return str.matches("[+-]?\\d*(\\.\\d+)?");
	}

	public static void main(String[] args) {
		String str = "123";
		System.out.println(str + "는 숫자입니까? " + isNumber(str));

		str = "123o";
		System.out.println(str + "는 숫자입니까? " + isNumber(str));

	}

}

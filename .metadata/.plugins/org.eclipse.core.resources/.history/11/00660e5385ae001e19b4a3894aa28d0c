package ch05;

import java.util.*;

public class practice09 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int input = 0;
		
		while(input!=5) {
			System.out.println("-------------------------------------------------------");
			System.out.println("1.학생수 | 2.점수입력 | 3. 점수리스트 | 4.분석 | 5.종료");
			System.out.println("-------------------------------------------------------");
			System.out.printf("선택> ");
			input = scan.nextInt();
		}

	}

}

package ch4;

import java.util.*;

public class Pass {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		//	출입증 입력
		int input = scan.nextInt();
		
		System.out.println("출입 가능한 연구실 :")
		//	각각의 케이스에 대한 출력
		switch(input) {
			case 101:
				System.out.println();
			case 102:
				System.out.println();
			case 103:
				System.out.println();
			case 201:
				System.out.println();
			case 202:
				System.out.println();
			case 301:
				System.out.println();
		}
	}
}

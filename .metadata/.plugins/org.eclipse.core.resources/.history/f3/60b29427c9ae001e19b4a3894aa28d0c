package ch06;

public class Car {
	String model = "현대자동차"; // 명시적 초기화
	int speed = 60;
	boolean start = true;

	{ // 초기화 블록
		model = "기아자동차";
		speed = 30;
	}

	Car() { // 생성자 순으로 값을 가져간다.
		model = "자동차";
	}
	
	Car(String m) {	//	파라미터의 개수가 같더라도 타입이 다르거나 순서가 다르면 오버로딩이 가능하다.
		model = m;
	}


	Car(int s) {
		speed = s;
	}

	Car(String m, int s, boolean st) {
		model = m;
		speed = s;
		start = st;
	}
}

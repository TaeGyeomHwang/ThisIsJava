package ch4;

import java.util.*;

public class MultipleCalculator {

	public static void main(String[] args) {
		//	값 입력
		Scanner scan = new Scanner(System.in);
		System.out.println("값을 입력해주세요. -> ");
		int input = scan.nextInt();
		
		//	3의 배수인가?
		if(input%3==0) {
			System.out.println("3의 배수이다.");
		}
		//	4의 배수인가?
		//	6의 배수인가?
		//	8의 배수인가?
	}

}

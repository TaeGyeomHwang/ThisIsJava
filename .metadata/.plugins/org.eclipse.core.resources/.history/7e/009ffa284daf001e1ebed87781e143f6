package ch06;

public class Rectangle {
	Rectangle() {
		System.out.println("Rectangle 객체를 생성하였습니다.");
	}

	int round(int a, int b) {
		System.out.println();
		return a * 2 + b * 2;
	}

	int area(int a, int b) {
		System.out.println();
		return a * b;
	}

	double area(double a, double b) {
		System.out.println();
		return a * b;
	}
}

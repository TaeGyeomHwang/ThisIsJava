package ch09;

public class Home {
//	RemoteControl rc = new RemoteControlImpl();
	
	//	위와 같은 동작을 하는 익명 객체
	RemoteControl rc = new RemoteControl() {
		@Override
		public void turnOn() {
			System.out.println("전원을 켭니다");
		}

		@Override
		public void turnOff() {
			System.out.println("전원을 끕니다");
		}
	};
}

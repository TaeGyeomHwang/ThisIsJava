package ch07;

public class SmartPhoneExam {

	public static void main(String[] args) {
		SmartPhone myPhone = new SmartPhone("아이폰", "검정", false);
		
		myPhone.model = "아이폰";
		myPhone.color = "검정";
		myPhone.wifi = false;
		
		System.out.println("모델: "+ myPhone.model + "\n색상: " + myPhone.color + "\n와이파이: " + myPhone.wifi);
		System.out.println();
	}

}

package ch3;

public class AccuravyExample {

	public static void main(String[] args) {
//		int apple = 1;
//		double pieceUnit = 0.1;	//	실수로는 정확한 연산이 불가능하다.
//		int number = 7;
//		double result = apple - number * pieceUnit;
		
		int apple = 1;
		int number = 7;
		
		double result = (apple*10 - number)/10.0;
		System.out.println(result);	
	}

}
